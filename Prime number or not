#first formula
n = int(input())
c = 0;
for i in range(n) :
    if n % (i+1)==0:
        c=c+1;
if c==2:
    print(n,"adalah bilangan prima ")
else :
    print(n,"adalah bukan bilangan prima ")


#Second formula

n = int(input())
cek=True
for i in range(2,n) :
    if n%i==0:
        cek=False
        break
if cek==True:
    print(n,"adalah bilangan prima ")
else :
    print(n,"adalah bukan bilangan prima ")


#third formula

n = int(input())
cek=True
for i in range(2,n) :
    if n%i==0:
        cek=False
        break
if cek==True:
    print(n,"adalah bilangan prima ")
else :
    print(n,"adalah bukan bilangan prima ")

